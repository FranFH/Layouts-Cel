<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="355"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="0">
            <shape
                android:shape="line"
                android:top="1dp">
                <stroke
                    android:width="35dp"
                    android:dashWidth="35dp"
                    android:color="#009688"/>

            </shape>




        </rotate>
    </item>
    <item
        android:top="65dp"
        android:left="2dp"
        >

        <rotate
            android:fromDegrees="355"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="0">
            <shape
                android:shape="line"
                android:top="1dp">
                <stroke
                    android:width="35dp"
                    android:dashWidth="35dp"
                    android:color="#F1238E"/>

            </shape>




        </rotate>
    </item>

</layer-list>